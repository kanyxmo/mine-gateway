{
    "name": "xmo/mine-gateway",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "mineadmin",
        "gateway"
    ],
    "description": "mineadmin app store gateway",
    "autoload": {
        "psr-4": {
            "Mine\\Gateway\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "xmo/mine-core": "2.0.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Mine\\Gateway\\ConfigProvider"
        }
    }
}
