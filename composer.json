{
    "name": "xmo/mine-gateway",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "mineadmin",
        "gateway"
    ],
    "description": "mineadmin app store gateway",
    "autoload": {
        "psr-4": {
            "Mine\\Gateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MineTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.tencent.com/composer/"
        }
    }
}
